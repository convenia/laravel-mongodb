{
    "name": "convenia/laravel-mongodb",
    "description": "A MongoDB based Eloquent model and Query builder for Laravel (Moloquent)",
    "keywords": [
        "laravel",
        "eloquent",
        "mongodb",
        "mongo",
        "database",
        "model",
        "moloquent"
    ],
    "homepage": "https://github.com/convenia/laravel-mongodb",
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "https://jenssegers.com"
        }
    ],
    "maintainers": [
        {
            "name": "Leonardo Lemos",
            "homepage": "http://l30.space"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^10.0",
        "illuminate/container": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/events": "^10.0",
        "mongodb/mongodb": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Convenia\\Mongodb\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    },
    "suggest": {
        "jenssegers/mongodb-sentry": "Add Sentry support to Laravel-MongoDB"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Convenia\\Mongodb\\MongodbServiceProvider",
                "Convenia\\Mongodb\\MongodbQueueServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
